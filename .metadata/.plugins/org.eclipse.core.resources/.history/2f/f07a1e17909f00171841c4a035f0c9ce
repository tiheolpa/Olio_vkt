//Koiraluokka
public class Dog {
	private String name;
	Dog(String nimi) {
		if (nimi == null) {
			name = "Doge";
		}
		else {
			name = nimi;
		}
		System.out.println("Hei, nimeni on " + name + "!");
	}

	public void speak(String lausahdus) {
		if (lausahdus == null) {
			System.out.println("Much wow!");
		}
		else {
			System.out.println(name + ": " + lausahdus);
		}
	}
}